unit uTagOwnerObj;

interface
uses
  uBaseObjStr, utag, ubaseobj;

  type
    cTagOwnerObj = class(cBaseObjStr)
    public
      tags:cBaseObjListStr;
    protected
      constructor create;override;
      destructor destroy;override;
      procedure createTags;virtual;
    end;

implementation

constructor cTagOwnerObj.create;
begin
  inherited;
  tags:=cBaseObjListStr.create;
  tags.destroydata:=true;
end;

destructor cTagOwnerObj.destroy;
var
  i:integer;
  tag:cbasetag;
begin
  for I := 0 to tags.Count - 1 do
  begin
    tag:=cBaseTag(tags.getobj(i));
    if Tag<>nil then
      tag.destroy;
  end;
  tags.Destroy;
  inherited;
end;

procedure cTagOwnerObj.createTags;
begin

end;


end.
