unit uMDB;

interface
uses
  uBaseObj;

type
  // база данных испытаний WP
  cMDB = class(cBaseObj)
    // список объектов базы данных
    ObjList:cBaseObj;
  public
    constructor Create;override;
  end;

const
  c_MDB_Test_image = 0;
  c_MDB_Obj_image = 1;
  c_MDB_Signals_image = 2;

implementation

constructor cMDB.Create;
begin
  inherited;
  ObjList:=cBaseObj.create;
end;

end.
